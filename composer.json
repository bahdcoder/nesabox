{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "aws/aws-sdk-php": "~3.0",
        "bahdcoder/digitalocean": "dev-master",
        "bahdcoder/linode": "@dev",
        "bahdcoder/vultr": "@dev",
        "beyondcode/laravel-websockets": "^1.3",
        "cviebrock/eloquent-sluggable": "^6.0",
        "dragonmantank/cron-expression": "^2.3",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "idimensionz/linode-api-v4": "dev-master",
        "laravel/envoy": "^1.5",
        "laravel/framework": "6.0.*",
        "laravel/helpers": "^1.1",
        "laravel/horizon": "^3.2",
        "laravel/nova": "*",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "protonemedia/laravel-paddle": "^1.0",
        "pusher/pusher-php-server": "^4.1",
        "spatie/laravel-activitylog": "^3.7",
        "spatie/laravel-cors": "^1.5",
        "thomaswelton/laravel-gravatar": "^1.1",
        "tymon/jwt-auth": "^1.0.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./vultr"
        },
        {
            "type": "path",
            "url": "./digitalocean"
        },
        {
            "type": "path",
            "url": "./linode"
        },
        {
            "type": "path",
            "url": "./nova"
        }
    ]
}
